
<div id='container'>
    {{> header}}
    <div id="addMovie">
            <h1>Create movie</h1>
            <form action="#/create" method="POST">
                <label>Title</label>
                <input type="text" name="title" id="createTitle" required>
                <label>Image Url</label>
                <input type="text" name="imageUrl" id="createImage" required>
                <label>Description</label>
                <textarea type="text" name="description" id="createDescription" required></textarea>
                <label>Genres</label>
                <input type="text" name="genres" id="createGenres" required>
                <label>Available Tickets</label>
                <input type="number" name="tickets" id="createTickets" required>
                <input type="submit" value="Create">
            </form>
        </div>
</div>

{{> footer}}
